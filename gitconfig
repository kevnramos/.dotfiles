[include]
    path = .gitlocal
[user]
    name = William Randall
    email = 15082470+randallwc@users.noreply.github.com
[filter "lfs"]
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true
[core]
    excludesfile = ~/.gitignore_global
    editor = vim
[color]
    ui = true
[diff]
    tool = vimdiff
[init]
    defaultBranch = main
[alias]
    a = add
    d = diff
    dc = diff --cached
    p = push
    c = commit
    cm = commit -m
    cam = commit -am
    camp = !git commit -am \"$1\" && git push && :
    co = checkout
    s = status --show-stash
    b = branch
    ls = status --short
    l = log --oneline
    ll = log --oneline --graph
