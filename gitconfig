[include]
    path = .gitlocal
[user]
    name = William Randall
    email = 15082470+randallwc@users.noreply.github.com
[filter "lfs"]
    clean = git-lfs clean -- %f
    smudge = git-lfs smudge -- %f
    process = git-lfs filter-process
    required = true
[core]
    excludesfile = ~/.gitignore_global
    editor = vim
[color]
    ui = true
[color "diff"]
    meta = yellow
    frag = cyan
    old = red
    new = green
[diff]
    tool = vimdiff
[init]
    defaultBranch = main
[alias]
    a = add
    aa = add .
    ai = add -i
    alias = !git config --get-regexp ^alias\\. | sed -e s/^alias\\.// -e s/\\ /\\ =\\ /
    ap = add -p
    b = branch
    branches = branch -a
    c = commit
    cam = commit -am
    camp = !git commit -am \"$1\" && git push && :
    cm = commit -m
    co = checkout
    d = diff
    dc = diff --cached
    f = fetch
    l = log --all
    lG = log --graph --decorate --name-status --all
    lg = log --oneline --graph --decorate --all
    lo = log --oneline
    p = push
    pp = pull
    remotes = remote -v
    s = status --show-stash
    ss = status --short
    sss = status --short --show-stash
